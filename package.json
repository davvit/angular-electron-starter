{
  "name": "angular-electron-starter",
  "productName": "AngularElectron",
  "version": "0.0.0",
  "description": "Angular 4 + Electron development, easier than ever!",
  "homepage": "https://github.com/resir014/angular-electron-starter",
  "author": "Resi Respati <resir014@blvd.space>",
  "license": "Unlicense",
  "keywords": [
    "angular",
    "electron",
    "typescript",
    "sass"
  ],
  "main": "./dist/electron/main.js",
  "private": true,
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "serve": "ng serve",
    "start": "ng build -w",
    "build:electron:main": "tsc -p ./tsconfig.electron.json --outDir dist/electron",
    "build": "npm run clean:dist && ng build && npm run build:electron:main",
    "build:prod": "npm run clean:dist && ng build --target=production && npm run build:electron:main",
    "clean": "npm run clean:dist && npm run clean:pkg",
    "clean:dist": "rimraf ./dist",
    "clean:pkg": "rimraf ./app-builds",
    "electron:serve": "npm run build:electron:main && electron . --serve",
    "electron:test": "electron .",
    "electron:dev": "npm run build && electron .",
    "electron:prod": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && npm run clean:pkg && node build/package.js --asar --platform=linux",
    "electron:windows": "npm run build:prod && npm run clean:pkg && node build/package.js --asar --platform=win",
    "electron:mac": "npm run build:prod && npm run clean:pkg && node build/package.js --asar --platform=darwin",
    "test": "karma start ./karma.conf.js",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet && npm run build",
    "e2e": "protractor ./protractor.conf.js"
  },
  "dependencies": {
    "@angular/common": "4.3.5",
    "@angular/compiler": "4.3.5",
    "@angular/core": "4.3.5",
    "@angular/forms": "4.3.5",
    "@angular/http": "4.3.5",
    "@angular/platform-browser": "4.3.5",
    "@angular/platform-browser-dynamic": "4.3.5",
    "@angular/router": "4.3.5",
    "@ngrx/effects": "^4.0.5",
    "@ngrx/store": "^4.0.3",
    "@ngrx/store-devtools": "^4.0.0",
    "core-js": "2.4.1",
    "enhanced-resolve": "3.3.0",
    "nedb": "^1.8.0",
    "ngrx-store-freeze": "^0.1.9",
    "normalize.css": "^5.0.0",
    "rxjs": "5.4.2",
    "zone.js": "0.8.12"
  },
  "devDependencies": {
    "@angular/cli": "^1.3.1",
    "@angular/compiler-cli": "4.3.5",
    "@types/core-js": "0.9.36",
    "@types/jasmine": "2.5.53",
    "@types/minimist": "^1.2.0",
    "@types/nedb": "^1.8.3",
    "@types/node": "7.0.7",
    "@types/webpack": "^3.0.10",
    "autoprefixer": "7.1.1",
    "codelyzer": "~3.1.1",
    "copyfiles": "^1.2.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "cssnano": "^3.10.0",
    "electron": "1.7.5",
    "electron-builder": "19.27.7",
    "electron-builder-squirrel-windows": "^19.28.3",
    "electron-reload": "1.2.1",
    "exports-loader": "^0.6.4",
    "file-loader": "^0.11.2",
    "html-loader": "^0.4.5",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "~2.6.4",
    "jasmine-spec-reporter": "~4.1.1",
    "json-loader": "^0.5.4",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-sourcemap-loader": "^0.3.7",
    "less-loader": "^4.0.4",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "postcss-loader": "^1.3.3",
    "postcss-url": "^7.0.0",
    "protractor": "^5.1.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "script-loader": "^0.7.0",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "stylus-loader": "^3.0.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.4.3",
    "typescript": "^2.5.0",
    "url-loader": "^0.5.9",
    "webdriver-manager": "12.0.6",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.5.0"
  }
}
